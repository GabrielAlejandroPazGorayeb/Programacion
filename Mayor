#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main()
{
	float A,B,C,Mayor;
	cout<<"Ingrese el primer numero";
	cin>>A;
	cout<<"Ingrese el segundo numero";
	cin>>B;
	cout<<"Ingrese el tercer valor";
	cin>>C;
	if (A>=B && A>=C)
		Mayor=A;
	if (B>=A && B>=C)
		Mayor=B;
	if (C>=A && C>=B)
		Mayor=C;
	cout<<"Su numero mayor es: ";
	cout<<Mayor;
	getch();
}

