#include "stdafx.h"
#include "conio.h"
#include <iostream>

using namespace std;

void main()
{
	float A;
	cout<<"Ingrese la edad";
	cin>>A;
	if (A>=18)
	cout<<"Puede votar";
	else
		cout<<"No puede votar";
	getch();
}
